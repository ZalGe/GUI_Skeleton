{
  "QtBinding": "PySide6",
  "ShowLogs": true,
  "LiveCompileQss": true,
  "CheckForMissingicons": false,
  "QMainWindow":
  [
    {
      "tittle": "gui_skeleton",
      "icon": "images/ZalGe_logo.png",
      "frameless": true,
      "translucentBg": true,
      "sizeGrip": "size_grip",
      "navigation":
      [
        {
          "minimize": "minimize_btn",
          "close": "close_btn",
          "moveWindow": "header",
          "titleBar": "header",
          "restore":
          [
            {
              "buttonName": "maximize_btn",
              "normalIcon": ":/feather/Icons/feather/square.png",
              "maximizedIcon": ":/feather/Icons/feather/copy.png"
            }
          ]
        }
      ]
    }
  ],
  "QSettings":
  [
    {
      "ThemeSettings":
      [
        {
           "CustomTheme": [
            {
              "Background-color": "#FFFFFF",
              "Text-color": "#010000",
              "Icons-color": "#000000",
              "Accent-color": "#26bae3",
              "Theme-name": "LightBlue",
              "Default-Theme": true
            },
            {
              "Background-color": "#21272a",
              "Text-color": "#fefefe",
              "Icons-color": "#fefefe",
              "Accent-color": "#fba43b",
              "Theme-name": "DarkYellow",
              "Default-Theme": false
            }
          ]
        }
      ],
      "AppSettings":
      {
        "OrginizationName": "ZalGe",
        "ApplicationName": "GUI skeleton",
        "OrginizationDormain": ""
      }
    }
  ],
  "QPushButtonGroup":
  [
      {
		"Buttons": [
		  "home_btn",
		  "create_json_btn",
		  "reports_btn",
		  "graphs_btn"
		],
		"ActiveButton": "home_btn",
		"Style": [
		  {
			"Active": "QPushButton{background-color: THEME.COLOR_BACKGROUND_2; font-weight: bold; border-top: 2px solid $COLOR_BACKGROUND_3; border-left: 5px solid $COLOR_BACKGROUND_3;border-bottom: 6px solid $COLOR_BACKGROUND_3;}",
			"NotActive": "QPushButton{background-color: transparent; font-weight: normal; border-top: 2px solid transparent; border-left: 5px solid transparent;border-bottom: 6px solid transparent;}"
		  }
		]
	  },
	  {
		"Buttons": [
		  "settings_btn",
		  "info_btn",
		  "help_btn",
		  "center_menu_close_btn"
		],
		"ActiveButton": "center_menu_close_btn",
		"Style": [
		  {
			"Active": "QPushButton{background-color: THEME.COLOR_ACCENT_1; font-weight: bold; border-top: 6px solid $COLOR_BACKGROUND_3; border-left: 6px solid $COLOR_BACKGROUND_3;border-bottom: 6px solid $COLOR_BACKGROUND_3;}",
			"NotActive": "QPushButton{background-color: transparent; font-weight: normal; border-top: 6px solid transparent; border-left: 6px solid transparent;border-bottom: 6px solid transparent;}"
		  }
		]
	  },
	  {
		"Buttons": [
		  "notification_btn",
		  "more_btn",
		  "profile_btn",
		  "right_menu_close_btn"
		],
		"Style": [
		  {
			"Active": "QPushButton{background-color: THEME.COLOR_BACKGROUND_2; font-weight: bold; border-radius: 5px} QPushButton::hover{background-color: THEME.COLOR_ACCENT_1}",
			"NotActive": "QPushButton{background-color: transparent; font-weight: normal; border-radius: 5px} QPushButton::hover{background-color: THEME.COLOR_ACCENT_1}"
		  }
		]
	  }
	],
  "QCustomSlideMenu": [
	  {
		"name": "left_menu",
		"defaultSize": [
		  {
			"width": 40,
			"height": "parent"
		  }
		],
		"collapsedSize": [
		  {
			"width": 40,
			"height": "parent"
		  }
		],
		"expandedSize": [
		  {
			"width": 130,
			"height": "parent"
		  }
		],
		"toggleButton": [
		  {
			"buttonName": "menu_btn",
			"icons": [
			  {
				"whenMenuIsCollapsed": ":/feather/icons/feather/align-justify.png",
				"whenMenuIsExpanded": ":/feather/icons/feather/arrow-left-circle.png"
			  }
			]
		  }
		],
		"menuTransitionAnimation": [
		  {
			"animationDuration": 500,
			"animationEasingCurve": "Linear"
		  }
		]
	  },
    {
      "name": "center_menu",
      "defaultSize": [
        {
        "width": 0,
        "height": "parent"
        }
      ],
      "collapsedSize": [
        {
        "width": 0,
        "height": "parent"
        }
      ],
      "expandedSize": [
        {
        "width": 200,
        "height": "parent"
        }
      ],
      "menuTransitionAnimation": [
        {
        "animationDuration": 500,
        "animationEasingCurve": "Linear"
        }
      ]
    },
    {
      "name": "right_menu",
      "defaultSize": [
        {
        "width": 0,
        "height": "parent"
        }
      ],
      "collapsedSize": [
        {
        "width": 0,
        "height": "parent"
        }
      ],
      "expandedSize": [
        {
        "width": 200,
        "height": "parent"
        }
      ],
      "menuTransitionAnimation": [
        {
        "animationDuration": 500,
        "animationEasingCurve": "Linear"
        }
      ]
    }
  ],
  "QCustomQStackedWidget": [
	  {
		"name": "main_pages",
		"navigation": [
		  {
			"navigationButtons": [
			  {
				"home_btn": "home_page",
        "create_json_btn": "create_json_page",
        "reports_btn": "reports_page",
        "graphs_btn": "graphs_page"
			  }
			]
		  }
		],
		"transitionAnimation": [
		  {
			"fade": [
			  {
				"active": true,
				"duration": 500,
				"easingCurve": "InSine"
			  }
			]
		  }
		]
	  },
	  {
		"name": "center_menu_pages",
		"navigation": [
		  {
			"navigationButtons": [
			  {
				"settings_btn": "settings_page",
				"info_btn": "info_page",
				"help_btn": "help_page"
			  }
			]
		  }
		],
    "transitionAnimation": [
		  {
			"fade": [
			  {
				"active": true,
				"duration": 500,
				"easingCurve": "InSine"
			  }
			],
      "slide": [
			  {
				"active": true,
				"duration": 500,
				"easingCurve": "InSine"
			  }
			]
		  }
		]
	  },
	  {
		"name": "right_menu_pages",
		"navigation": [
		  {
			"navigationButtons": [
			  {
				"notification_btn": "notification_page",
				"more_btn": "more_page",
				"profile_btn": "profile_page"
			  }
			]
		  }
		],
    "transitionAnimation": [
		  {
			"slide": [
			  {
				"active": true,
				"duration": 500,
				"easingCurve": "InSine"
			  }
			],
      "fade": [
			  {
				"active": true,
				"duration": 500,
				"easingCurve": "InSine"
			  }
			]
		  }
		]
	  }
	]
}
